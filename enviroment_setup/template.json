{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "workspaces_amlmlops1_name": {
            "defaultValue": "amlmlops1",
            "type": "String"
        },
        "storageAccounts_amlmlops18502245988_externalid": {
            "defaultValue": "/subscriptions/dac0efb4-5641-4925-8058-1584af5ae262/resourceGroups/amlmlops/providers/Microsoft.Storage/storageAccounts/amlmlops18502245988",
            "type": "String"
        },
        "vaults_amlmlops15513561655_externalid": {
            "defaultValue": "/subscriptions/dac0efb4-5641-4925-8058-1584af5ae262/resourceGroups/amlmlops/providers/Microsoft.Keyvault/vaults/amlmlops15513561655",
            "type": "String"
        },
        "components_amlmlops18053562212_externalid": {
            "defaultValue": "/subscriptions/dac0efb4-5641-4925-8058-1584af5ae262/resourceGroups/amlmlops/providers/Microsoft.insights/components/amlmlops18053562212",
            "type": "String"
        }
    },
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.MachineLearningServices/workspaces",
            "apiVersion": "2024-01-01-preview",
            "name": "[parameters('workspaces_amlmlops1_name')]",
            "location": "eastus",
            "sku": {
                "name": "Basic",
                "tier": "Basic"
            },
            "kind": "Default",
            "identity": {
                "type": "SystemAssigned"
            },
            "properties": {
                "friendlyName": "[parameters('workspaces_amlmlops1_name')]",
                "storageAccount": "[parameters('storageAccounts_amlmlops18502245988_externalid')]",
                "keyVault": "[parameters('vaults_amlmlops15513561655_externalid')]",
                "applicationInsights": "[parameters('components_amlmlops18053562212_externalid')]",
                "hbiWorkspace": false,
                "managedNetwork": {
                    "isolationMode": "Disabled"
                },
                "v1LegacyMode": false,
                "publicNetworkAccess": "Enabled",
                "ipAllowlist": [],
                "discoveryUrl": "https://eastus.api.azureml.ms/discovery",
                "EnableSoftwareBillOfMaterials": false,
                "enableDataIsolation": false
            }
        },
        {
            "type": "Microsoft.MachineLearningServices/workspaces/datastores",
            "apiVersion": "2024-01-01-preview",
            "name": "[concat(parameters('workspaces_amlmlops1_name'), '/workspaceartifactstore')]",
            "dependsOn": [
                "[resourceId('Microsoft.MachineLearningServices/workspaces', parameters('workspaces_amlmlops1_name'))]"
            ],
            "properties": {
                "credentials": {
                    "credentialsType": "AccountKey",
                    "secrets": {}
                }
            }
        },
        {
            "type": "Microsoft.MachineLearningServices/workspaces/datastores",
            "apiVersion": "2024-01-01-preview",
            "name": "[concat(parameters('workspaces_amlmlops1_name'), '/workspaceblobstore')]",
            "dependsOn": [
                "[resourceId('Microsoft.MachineLearningServices/workspaces', parameters('workspaces_amlmlops1_name'))]"
            ],
            "properties": {
                "credentials": {
                    "credentialsType": "AccountKey",
                    "secrets": {}
                }
            }
        },
        {
            "type": "Microsoft.MachineLearningServices/workspaces/datastores",
            "apiVersion": "2024-01-01-preview",
            "name": "[concat(parameters('workspaces_amlmlops1_name'), '/workspacefilestore')]",
            "dependsOn": [
                "[resourceId('Microsoft.MachineLearningServices/workspaces', parameters('workspaces_amlmlops1_name'))]"
            ],
            "properties": {
                "credentials": {
                    "credentialsType": "AccountKey",
                    "secrets": {}
                }
            }
        },
        {
            "type": "Microsoft.MachineLearningServices/workspaces/datastores",
            "apiVersion": "2024-01-01-preview",
            "name": "[concat(parameters('workspaces_amlmlops1_name'), '/workspaceworkingdirectory')]",
            "dependsOn": [
                "[resourceId('Microsoft.MachineLearningServices/workspaces', parameters('workspaces_amlmlops1_name'))]"
            ],
            "properties": {
                "credentials": {
                    "credentialsType": "AccountKey",
                    "secrets": {}
                }
            }
        }
    ]
}